component PrefixFile(prefix, file) -> out {
  filename : std.Constant(data=file)
  file : io.FileReader()
  prep : std.PrefixString(prefix=prefix)

  filename.token > file.filename
  file.out > prep.in
  prep.out >out
}

src : PrefixFile(prefix="?--- ", file="data.txt")
snk : io.StandardOut()

src.out>snk.token
