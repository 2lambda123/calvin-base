// Nested components 2

component E() in -> out {
  f:std.Identity()
  g:std.Identity()

  .in > f.token
  f.token > g.token
  g.token > .out
}

component B() in -> out {
  e:E()

  .in > e.in
  e.out > .out
}

a:std.Counter()
b:B()
c:test.Sink(store_tokens=false, active=true, quiet=false)

a.integer > b.in
b.out > c.token
